{
  "headers": [
    {
      "source": "/manifest.webmanifest",
      "headers": [
        { "key": "Content-Type", "value": "application/manifest+json" }
      ]
    }
  ],
  "functions": {
    "api/admin.js": {
      "maxDuration": 300
    }
  },
  "rewrites": [
    { "source": "/api/admin/(.*)", "destination": "/api/admin?action=$1" },
    { "source": "/api/(.*)", "destination": "/api/$1" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
